{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
      {
          "src": "./app.py",
          "use": "@vercel/python"
      }
  ],
  "installCommand": "pip install -r requirements.txt -t ./",
  "routes": [
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ]
}